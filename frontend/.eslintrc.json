{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "object-shorthand": 1,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-use-before-define": 0,
        "no-console": 0,
        "react/forbid-prop-types": 0,
        "no-param-reassign": 0,
        "indent": 0,
        "max-len": 0,
        "react/jsx-indent": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "react/no-typos": 0,
        "react/no-array-index-key": 0,
        "object-curly-newline": 0,
        "object-property-newline": 0,
        "no-shadow": 0
    },
    "globals": {
        "window": true,
        "document": true,
        "navigator": true,
        "registration": true,
        "fetch": true,
        "URL": true
    }
}
